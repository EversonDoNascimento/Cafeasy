{
  "expo": {
    "name": "cafeasy",
    "slug": "cafeasy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-coffee.png",
    "scheme": "cafeasy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-coffee.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.everson25nascimento.cafeasy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon-coffee.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon-coffee.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "877db5b8-56af-42c6-a972-59fc709c5b80"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/877db5b8-56af-42c6-a972-59fc709c5b80"
    }
  }
}
